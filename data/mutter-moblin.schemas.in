<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/desktop/moblin/toolbar/panels/order</key>
      <applyto>/desktop/moblin/toolbar/panels/order</applyto>
      <owner>mutter-moblin</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[moblin-panel-myzone,moblin-panel-zones,moblin-panel-applications,moblin-panel-status,moblin-panel-people,moblin-panel-internet,meego-panel-banshee,moblin-panel-devices,moblin-power-icon,moblin-panel-datetime,carrick,bluetooth-panel]</default>
      <locale name="C">
         <short>Order of panels on toolbar</short>
         <long>
		Determines the order of panels on the toolbar.
         </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/moblin/always_small_screen_mode</key>
      <applyto>/desktop/moblin/always_small_screen_mode</applyto>
      <owner>mutter-moblin</owner>
      <type>bool</type>
      <default>FALSE</default>
      <locale name="C">
         <short>Always use small screen mode</short>
         <long>
		Forces small screen mode to be used even if display size would
                normally imply bigger screen mode.
         </long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
